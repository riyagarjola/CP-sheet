#include<bits/stdc++.h>
using namespace std;
 
int main(){
string s;
cin >> s;
 
set<char> st;
for(char c:s){
st.insert(c);
}
 
int n=st.size();
if(n%2==0){
cout<<"CHAT WITH HER!"<<endl;
}
else{
cout<<"IGNORE HIM!"<<endl;
}
 
return 0;
}
-------------------------------------------------------------------------------------------------------------------------------------------------------------------
#include<bits/stdc++.h>
using namespace std;
 
int main(){
string s;
cin >> s;
 
bool seen[26]={false};
 
int count=0;
 
for(char c:s){
if(!seen[c-'a']){
seen[c-'a']=true;
count++;
}
}
 
if(count%2==0){
cout<<"CHAT WITH HER!"<<endl;
}
else{
cout<<"IGNORE HIM!"<<endl;
}
 
return 0;
}
